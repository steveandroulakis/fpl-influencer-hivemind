# Required for YouTube discovery
YOUTUBE_API_KEY=your-youtube-data-api-key

# Required for running the Anthropic analysis pipeline
ANTHROPIC_API_KEY=your-anthropic-api-key

# Optional FPL authentication (for accurate selling prices)
# Without these, selling_price defaults to current market price
FPL_EMAIL=
FPL_PASSWORD=

# Primary transcript provider (youtube-transcript.io)
YOUTUBE_TRANSCRIPT_IO_KEY=

# Optional transcript provider (EasySubAPI)
RAPIDAPI_EASYSUB_API_KEY=

# Optional cookies path for yt-dlp transcript fallback
YOUTUBE_COOKIES_PATH=

# Optional override: set to "existing" to skip youtube-transcript.io entirely
TRANSCRIPT_FETCHER_PREFERENCE=auto
